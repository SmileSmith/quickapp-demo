    { "name": "system.shortcut" },
    { "name": "system.storage" },
    { "name": "system.fetch" },
    { "name": "system.webview" }
  ],
  "permissions": [{ "origin": "*" }],
  "config": {
    "logLevel": "info",
    "designWidth": 375
  },
  "router": {
    "entry": "pages/Home",
    "pages": {
      "pages/Home": {
        "component": "index"
      },
      "pages/Weibo": {
        "component": "index"
      },
